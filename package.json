  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-duarte/stats-preview-card-component-main.git"
  },
  "author": "Victor Duarte (@victor-duarte)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victor-duarte/stats-preview-card-component-main/issues"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/react-dom": "^17.0.5",
    "prettier": "2.3.0"
  },
  "dependencies": {
    "node-sass": "^6.0.0",
